#!/bin/sh

echo "Start in a screen session."
echo "Once server has started, disconnect from screen with Ctrl-A d"
echo
echo "Then connect from your computer to run imports etc. E.g."
echo "lein repl :connect saturn:3001"
echo "(clojure-photo-bank.photo-store/import-images!)"
echo

cd ~/code/clojure/clojure-photo-bank/
lein run
